    "configurations": [
        {
            "name": "Python: Run Off-Policy",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "args": [
                "--alg", "off_pg",
                "--env", "half_cheetah",
                "--n_workers", "10",
                "--var", "0.1",
                "--horizon", "200",
                "--ite", "1000",
                "--n_trials", "1",
