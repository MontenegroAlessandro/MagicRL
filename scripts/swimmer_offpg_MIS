python3 run.py --alg pg --env swimmer --n_workers 10 --var 0.3 --horizon 200 --ite 5000 --n_trials 5 --clip 0 --lr 0.003 --pol 'nn' --dir results/swimmer_MIS_nn/equal_batchxwindow/

python3 run.py --alg off_pg --env swimmer --n_workers 10 --var 0.3 --horizon 200 --ite 5000 --window_length 2 --n_trials 5 --clip 0 --lr 0.003 --batch 50 --pol 'nn' --weight_type 'MIS' --dir results/swimmer_MIS_nn/equal_batchxwindow/

python3 run.py --alg off_pg --env swimmer --n_workers 10 --var 0.3 --horizon 200 --ite 5000 --window_length 4 --n_trials 5 --clip 0 --lr 0.003 --batch 25 --pol 'nn' --weight_type 'MIS' --dir results/swimmer_MIS_nn/equal_batchxwindow/

python3 run.py --alg off_pg --env swimmer --n_workers 10 --var 0.3 --horizon 200 --ite 5000 --window_length 10 --n_trials 5 --clip 0 --lr 0.003 --batch 10 --pol 'nn' --weight_type 'MIS' --dir results/swimmer_MIS_nn/equal_batchxwindow/